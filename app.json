{
  "expo": {
    "name": "radio-mobile",
    "slug": "radio-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "radiomobile",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.aartilather.radiomobile",
      "permissions": [
        "INTERNET",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK",
        "VIBRATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "4fa5705f-1f6c-490a-9cf8-be64930ac612"
      }
    }
  }
}
